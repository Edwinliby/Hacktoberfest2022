<!-- Java program to add number from the string-->
class extractinteger{
	
	public static void main(String args[])
	{
		String str="abcdef45fhgjdj95kkmk";
		String num="";
		int sum=0;
		
		for(int i=0;i<str.length();i++)
		{
			if(Character.isDigit(str.charAt(i)))
			{
				num=num+str.charAt(i);	
			}
			else{
				if(!num.equals(""))
				{
				sum=sum+Integer.parseInt(num);
				num="";

				}

			}
			
		}
		System.out.println("Summation of the string digits are "+sum);		

	}

}
